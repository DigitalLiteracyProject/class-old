<div class="row">
  <div class="col-lg-12">
    <h1>Classrooms<small></small></h1>
    <div class="list-group">
        <% classrooms.forEach(function(classroom){ %>
            <li class="list-group-item"><a href="/classroom/<%= classroom.id %>/show"><%= classroom.name %></a> <span class="pull-right"><a href="/classroom/<%= classroom.id %>/show" data-title="Show" data-placement="left" class="show-icon"><i class="fa fa-eye"></i></a> <a href="/classroom/<%= classroom.id %>/edit" data-placement="right" data-title="Edit" class="delete-icon"><i class="fa fa-edit"></i></a></span></li>
        <% }); %>
    </div>
    <a class="btn btn-success" href="/classroom/create"><i class="fa fa-plus-square-o fa-lg"></i> New classroom</a>
  </div>
</div>